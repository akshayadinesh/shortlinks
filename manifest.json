{
  "name": "Go//To",
  "description" : "Type to/[phrase] to access one of your shortcuts.",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "omnibox": { "keyword" : "to/" },
  "permissions": [
  	"activeTab",
  	"tabs",
  	"webRequest",
  	"webRequestBlocking"
  ],
  "manifest_version": 2
}
